{
  "name": "server",
  "version": "1.0.0",
  "description": "UGB Student Portal API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only server/src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:studio": "npx prisma studio --schema=server/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=server/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=server/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "5.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}